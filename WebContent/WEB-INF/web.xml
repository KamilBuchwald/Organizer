   <servlet-mapping>
  	<servlet-name>RegistryServlet</servlet-name>
  	<url-pattern>/registry</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>ValidServlet</servlet-name>
