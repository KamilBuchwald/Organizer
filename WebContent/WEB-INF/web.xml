  </servlet>
  
  <servlet>
  	<servlet-name>SettingsServlet</servlet-name>
  	<servlet-class>servlets.SettingsServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>CalculateFinalServlet</servlet-name>
  	<servlet-class>servlets.CalculateFinalServlet</servlet-class>
  </servlet>
  
   <servlet>
  	<servlet-name>CurrencyCheckServlet</servlet-name>
  	<servlet-class>servlets.CurrencyCheckServlet</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>LogoutServlet</servlet-name>
  	<servlet-class>servlets.LogoutServlet</servlet-class>
  </servlet>
  
  
  <servlet-mapping>
